 <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id = "@+id/RHE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="-10dp"
            android:padding="5dp">

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/dateff"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/projectff"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/vendorff"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/teamsff"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/total_teamsff"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/dcff"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/hvff"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/workff"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/unknownff"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/dc_completeff"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/hv_completeff"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/remarksff"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:padding="5dp"
                    android:text="@string/remarks"
                    android:textAlignment="center" />
            </TableRow>
        </TableLayout>
